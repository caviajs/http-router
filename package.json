{
  "name": "@caviajs/http-router",
  "description": "a micro framework for node.js",
  "version": "1.2.4",
  "author": "PaweÅ‚ Partyka",
  "license": "MIT",
  "repository": "caviajs/http-router",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register jest --runInBand",
    "test:watch": "jest --watch",
    "release": "npm run build && npm publish",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "devDependencies": {
    "@caviajs/http-exception": "^1.0.5",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.3",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "release-it": "^17.0.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "rxjs": "^7.8.1"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "test"
    ],
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
